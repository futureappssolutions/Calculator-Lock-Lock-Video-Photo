<resources>

    <style name="Theme.CalculatorLockVideoPhoto" parent="@style/Theme.AppCompat.Light.NoActionBar">
        <item name="android:actionOverflowButtonStyle">@style/OverFlow</item>
        <item name="colorAccent">@color/app_color</item>
        <item name="colorPrimary">@color/tool_color</item>
        <item name="colorPrimaryDark">@color/tool_color</item>
    </style>

    <style name="OverFlow" parent="@android:style/Widget.Holo.ActionButton.Overflow">
        <item name="android:src">@drawable/ic_more_top_bar_icon</item>
    </style>

    <style name="menu_labels_style">
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@drawable/shape_app_color</item>
        <item name="android:textSize">@dimen/_10ssp</item>
        <item name="android:fontFamily">@font/font_medium</item>
        <item name="android:paddingStart">@dimen/_10sdp</item>
        <item name="android:paddingEnd">@dimen/_10sdp</item>
        <item name="android:paddingTop">@dimen/_3sdp</item>
        <item name="android:paddingBottom">@dimen/_3sdp</item>
    </style>

    <style name="ThemeOverlay_YourApp" parent="@style/ThemeOverlay.AppCompat.Light">
        <item name="android:colorBackground">@color/ColorWhite</item>
        <item name="android:textColor">@color/ColorAppTheme</item>
        <item name="colorPrimary">@color/ColorPrimary</item>
        <item name="colorPrimaryDark">@color/ColorPrimaryDark</item>
    </style>

    <style name="PickerEditText">
        <item name="android:textSize">22sp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">4dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:imeOptions">actionNext</item>
    </style>

    <style name="CustomTextInputLayoutStyle_ErrorTextStyle" parent="@style/TextAppearance.Design.Error">
        <item name="android:textSize">15sp</item>
    </style>

    <style name="CustomTextInputLayoutStyle_HintTextStyle" parent="@style/TextAppearance.Design.Hint">
        <item name="android:textSize">15sp</item>
    </style>

    <style name="AppTheme123" parent="@style/Theme.AppCompat.Light.DarkActionBar">
        <item name="android:itemBackground">@color/overflow_background</item>
    </style>

    <style name="FullHeightDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="AppTheme_Dark" parent="@style/Theme.AppCompat.NoActionBar">
        <item name="backspace">@drawable/ic_backspace_white_24dp</item>
        <item name="panel1">@color/panel_1_dark</item>
        <item name="panel2">@color/panel_2_dark</item>
    </style>

    <style name="AppTheme_Dark_Blue" parent="@style/AppTheme_Dark">
        <item name="colorPrimary">@color/blue</item>
        <item name="colorPrimaryDark">@color/black_color</item>
    </style>

    <style name="AppTheme_Dark_Cyan" parent="@style/AppTheme_Dark">
        <item name="colorPrimary">@color/cyan</item>
        <item name="colorPrimaryDark">@color/cyan_dark</item>
    </style>

    <style name="AppTheme_Dark_Gray" parent="@style/AppTheme_Dark">
        <item name="colorPrimary">@color/gray</item>
        <item name="colorPrimaryDark">@color/gray_dark</item>
    </style>

    <style name="AppTheme_Dark_Green" parent="@style/AppTheme_Dark">
        <item name="colorPrimary">@color/green</item>
        <item name="colorPrimaryDark">@color/green_dark</item>
    </style>

    <style name="AppTheme_Dark_Indigo" parent="@style/AppTheme_Dark">
        <item name="colorPrimary">@color/indigo</item>
        <item name="colorPrimaryDark">@color/indigo_dark</item>
    </style>

    <style name="AppTheme_Dark_Purple" parent="@style/AppTheme_Dark">
        <item name="colorPrimary">@color/purple</item>
        <item name="colorPrimaryDark">@color/purple_dark</item>
    </style>

    <style name="AppTheme_Dark_Red" parent="@style/AppTheme_Dark">
        <item name="colorPrimary">@color/red</item>
        <item name="colorPrimaryDark">@color/red_dark</item>
    </style>

    <style name="AppTheme_Light" parent="@style/Theme.AppCompat.Light.NoActionBar">
        <item name="backspace">@drawable/ic_backspace_black_24dp</item>
        <item name="panel1">@color/panel_1_light</item>
        <item name="panel2">@color/panel_2_light</item>
    </style>

    <style name="AppTheme_Light_Blue" parent="@style/AppTheme_Light">
        <item name="colorPrimary">@color/blue</item>
        <item name="colorPrimaryDark">@color/black_color</item>
    </style>

    <style name="AppTheme_Light_Cyan" parent="@style/AppTheme_Light">
        <item name="colorPrimary">@color/cyan</item>
        <item name="colorPrimaryDark">@color/cyan_dark</item>
    </style>

    <style name="AppTheme_Light_Gray" parent="@style/AppTheme_Light">
        <item name="colorPrimary">@color/gray</item>
        <item name="colorPrimaryDark">@color/gray_dark</item>
    </style>

    <style name="AppTheme_Light_Green" parent="@style/AppTheme_Light">
        <item name="colorPrimary">@color/green</item>
        <item name="colorPrimaryDark">@color/green_dark</item>
    </style>

    <style name="AppTheme_Light_Indigo" parent="@style/AppTheme_Light">
        <item name="colorPrimary">@color/indigo</item>
        <item name="colorPrimaryDark">@color/indigo_dark</item>
    </style>

    <style name="AppTheme_Light_Purple" parent="@style/AppTheme_Light">
        <item name="colorPrimary">@color/purple</item>
        <item name="colorPrimaryDark">@color/purple_dark</item>
    </style>

    <style name="AppTheme_Light_Red" parent="@style/AppTheme_Light">
        <item name="colorPrimary">@color/red</item>
        <item name="colorPrimaryDark">@color/red_dark</item>
    </style>
</resources>